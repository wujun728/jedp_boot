    <select id="selectEmpDeptInfo" resultType="cn.kiwipeach.demo.domain.dto.EmployDeptDTO">
        select e.empno,
               e.ename,
               e.job,
               e.mgr,
               decode(e.mgr, e.mgr, (select ename from emp where empno = e.mgr)) mgrname,
               d.deptno,
               d.dname,
               d.LOC
      from emp e, dept d
     where e.deptno = d.deptno
       and e.empno = #{empno}
    </select>

